{
    "name": "roadiz/rozier-bundle",
    "license": "MIT",
    "keywords": [
        "cms",
        "backoffice",
        "rezo zero"
    ],
    "authors": [
        {
            "name": "Ambroise Maupate",
            "email": "ambroise@roadiz.io",
            "homepage": "https://www.roadiz.io",
            "role": "Lead developer"
        }
    ],
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "5.4.*",
        "roadiz/core-bundle": "~2.0.36 || dev-develop",
        "roadiz/rozier": "~2.0.12 || dev-develop",
        "roadiz/compat-bundle": "~2.0.0 || dev-develop",
        "roadiz/openid": "~2.0.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.4",
        "phpstan/phpstan": "^1.5.3",
        "phpstan/phpstan-symfony": "^1.1.8",
        "phpstan/phpstan-doctrine": "^1.3.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": false,
            "symfony/runtime": false
        }
    },
    "autoload": {
        "psr-4": {
            "RZ\\Roadiz\\RozierBundle\\": "src/"
        },
        "files": [
            "deprecated.php"
        ]
    }
}
