{% embed "@RoadizRozier/macros/rz_actions_menu.html.twig" with {
    vertical: true,
    class: 'actions-menu',
    item: item
} only %}
    {% block items %}
        {% if is_granted('ROLE_ACCESS_GROUPS') and is_granted(item) and item.id %}
            {{ _self.deleteItem(href: path('groupsDeletePage', { id: item.getId})) }}
        {% endif %}
    {% endblock %}
{% endembed %}
