{% if with_breadcrumb is defined and with_breadcrumb %}
    {% set parents = [
        {
            label: 'groups.homepage'|trans,
            type: 'listing',
            url: path('groupsHomePage'),
        }
    ] %}
    {% for parent in breadcrumb_parents %}
        {% set parents = parents|merge([{
            label: parent.displayName,
            type: parent.type,
            url: parent.url,
        }]) %}
    {% endfor %}
    {% set breadcrumb = {
        label: 'groups-breadcrumb',
        parent: parents,
        current: title
    } %}
{% endif %}
{% include "@RoadizRozier/admin/head.html.twig" with {
    nav_bar_template: '@RoadizRozier/groups/navBar.html.twig',
    nav_bar_context: {'item': item, current: current}
} %}
