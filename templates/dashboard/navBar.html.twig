{%
    set listItems = [
    [
        {
            'name': 'dashboard',
            'label': 'dashboard',
            'href': path('adminHomePage'),
        },
        is_granted('ROLE_ACCESS_LOGS') ? {
            'name': 'history',
            'label': 'history',
            'href': path('historyHomePage'),
        } : null,
    ]
]
%}
{% set custom_entries_content %}
