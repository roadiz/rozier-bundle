{%
set listItems = [
    [
        {
            'name': 'customForm',
            'label': 'custom-form.parameters',
            'href': path('customFormsEditPage', { id: type.id }),
        },
        is_granted('ROLE_ACCESS_USERS') ? {
            'name': 'webhook',
            'label': 'custom-form.webhook',
            'href': path('customFormsEditWebhookPage', { customForm: type.id }),
        } : null,
        {
            'name': 'usage',
            'label': 'custom-form.usage',
            'href': path('customFormsUsagePage', { id: type.id }),
        },
        {
            'name': 'customFormFields',
            'label': 'custom-form.fields',
            'href': path('customFormFieldsListPage', { customFormId: type.id }),
        },
        {
            'name': 'customFormAnswers',
            'label': 'custom-form.answers',
            'href': path('customFormAnswersHomePage', { customFormId: type.id }),
        },
    ],
    [
        {
            'name': 'customForm.show',
            'href': path('customFormSendAction', { customFormId: type.id }),
            'icon': 'rz-icon-ri--eye-line',
            'target': '_blank',
            'iconOnly': true,
        },
    ]
]
%}

{% set custom_entries_content %}
    {% block additional_entries %}{% endblock %}
{% endset %}

{% embed "@RoadizRozier/macros/rz_menu_bar.html.twig" with {listItems : listItems, current: current} %}
    {% block additional_entries %}
        {{ custom_entries_content|raw }}
    {% endblock %}
{% endembed %}
